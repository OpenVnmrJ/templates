<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Shim" element="pages" type="sample" >
  <group size="800 232" style="PlainText" label="Shim"
    vc="sethw('statrate',400):$x"
    vc2="sethw('statrate',5000):$x"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    reference="Shim"
    useref="no"
    expanded="yes"
    >
    <group loc="8 0" size="80 208" style="PlainText"
      vq="acqmode"
      show="if ((acqmode = 'fidscan') or (acqmode='auscan')) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      useref="no"
      >
      <toggle loc="0 104" size="80 24" style="Label4"
        label="Imaginary"
        vq="displaymode"
        vc="displaymode('+i')"
        vc2="displaymode('-i')"
        set="strstr(displaymode,'i'):$VALUE"
        radiobutton="false"
        />
      <toggle loc="0 80" size="80 24" style="Label4"
        label="Real"
        vq="displaymode"
        vc="displaymode('+r')"
        vc2="displaymode('-r')"
        set="strstr(displaymode,'r'):$VALUE"
        radiobutton="false"
        />
      <radio loc="0 56" size="80 24" style="Label4"
        label="FID"
        vq="displaymode"
        vc="displaymode('-s')"
        set="strstr(displaymode,'s'):$e $VALUE=(not $e)"
        />
      <shimbutton loc="0 0" size="80 32" style="Label4"
        label="z0"
        vc="setshim('z0',$VALUE)"
        vq2="z0"
        statpar="z0"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
      <toggle loc="0 176" size="80 24" style="Label4"
        label="Dots"
        vq="dotflag"
        vc="dotflag='y'"
        vc2="dotflag='n'"
        set="$VALUE=(dotflag = 'y')"
        radiobutton="false"
        />
      <toggle loc="0 152" size="80 24" style="Label4"
        label="Envelope"
        vq="displaymode"
        vc="displaymode('+e')"
        vc2="displaymode('-e')"
        set="strstr(displaymode,'e'):$VALUE"
        radiobutton="false"
        />
      <toggle loc="0 128" size="80 24" style="Label4"
        label="Abs Value"
        vq="displaymode"
        vc="displaymode('+a')"
        vc2="displaymode('-a')"
        set="strstr(displaymode,'a'):$VALUE"
        radiobutton="false"
        />
      <radio loc="0 32" size="80 24" style="Label4"
        label="Spectrum"
        vq="displaymode"
        vc="displaymode('+s')"
        set="strstr(displaymode,'s'):$VALUE"
        />
    </group>
    <group loc="0 0" size="96 136" style="PlainText"
      vq="acqmode"
      show="if ((acqmode &lt;&gt; 'fidscan') and (acqmode&lt;&gt;'auscan')) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      useref="no"
      >
      <shimbutton loc="8 96" size="80 32" style="Label4"
        label="Lk Phase"
        vc="setshim('lockphase',$VALUE)"
        min="0"
        max="360"
        statpar="lockphase"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
      <shimbutton loc="8 0" size="80 32" style="Label4"
        label="z0"
        vc="setshim('z0',$VALUE)"
        vq2="z0"
        statpar="z0"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
      <shimbutton loc="8 32" size="80 32" style="Label4"
        label="Lk Power"
        vc="setshim('lockpower',$VALUE)"
        vq2="lockpower"
        statpar="lockpower"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
      <shimbutton loc="8 64" size="80 32" style="Label4"
        label="Lk Gain"
        vc="setshim('lockgain',$VALUE)"
        vq2="lockgain"
        statpar="lockgain"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
    </group>
    <label loc="456 176" size="72 24" style="Label4"
      label="Recr gain"
      justify="Left"
      />
    <entry loc="456 200" size="72 24" style="Label4"
      vq="gain acqmode"
      vc="gain = $VALUE"
      set="$VALUE = gain"
      show="$VALUE = (acqmode &lt;&gt; 'fidscan')*2 - 1"
      digits="0"
      disable="Label"
      />
    <shimbutton loc="360 192" size="80 32" style="Label4"
      label="Spin"
      vc="setshim('spin',$VALUE)"
      vq2="spin"
      statpar="spin"
      pointy="false"
      rocker="true"
      arrow="true"
      arrowcolor="GraphForeground"
      wrap="false"
      />
    <button loc="272 200" size="80 24" style="Label4"
      label="Spin Off"
      vc="sethw('spinner','off')"
      />
    <button loc="272 176" size="80 24" style="Label4"
      label="Spin On"
      vc="sethw('spinner','on')"
      />
    <toggle loc="8 208" size="80 24" style="Label4"
      label="DC Correct"
      vq="dcrmv"
      vc="dcrmv='y' fidarea:fidarea"
      vc2="dcrmv='n' fidarea:fidarea"
      set="if (dcrmv = 'y') then $VALUE=1 else $VALUE=0 endif"
      radiobutton="false"
      />
    <dial loc="96 0" size="160 176" style="Label4"
      digital="yes"
      min="0.0"
      max="100.0"
      minmark="no"
      maxmark="yes"
      statpar="lklvl"
      count="2"
      color1="PlainText"
      elastic="no"
      />
    <textmessage loc="176 176" size="80 24" style="Label4"
      label="0.4"
      statpar="lklvl"
      />
    <group loc="112 200" size="144 24" style="PlainText"
      vq="acqmode"
      show="$VALUE = (acqmode = 'fidscan')"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      useref="no"
      >
      <textmessage loc="64 0" size="72 24" style="Label4"
        label="110.3129"
        vq="fidarea"
        set="$VALUE = fidarea"
        digits="4"
        />
      <label loc="0 0" size="64 24" style="Label4"
        label="FID area"
        justify="Left"
        />
    </group>
    <label loc="112 176" size="64 24" style="Label4"
      label="Lock"
      justify="Left"
      />
    <button loc="568 184" size="224 32" style="Heading4"
      label="Save Shims"
      vc="save_shims"
      />
    <shimset loc="264 0" size="528 176" style="PlainText"
      vq="shimset"
      vc="setshim('$NAME',$VALUE)"
      set="$VALUE=shimset"
      border="LoweredBevel"
      side="Top"
      justify="Left"
      tab="no"
      savekids="false"
      statpar="$NAME"
      pointy="false"
      rocker="true"
      arrow="true"
      reference="shm"
      useref="no"
      expanded="yes"
      />
  </group>
</template>
