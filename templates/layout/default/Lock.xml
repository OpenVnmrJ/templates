<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Lock" element="pages" type="sample" >
  <group size="800 200" style="PlainText" label="Lock"
    vq="system"
    vc="sethw('statrate',200):$x"
    vc2="sethw('statrate',5000):$x"
    show="$VALUE = (system='spectrometer')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    reference="Lock"
    useref="no"
    expanded="yes"
    >
    <group loc="24 80" size="120 64" style="PlainText"
      vq="system"
      show="is_spectrometer('value'):$ENABLE"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      useref="no"
      >
      <label loc="0 0" size="112 32" style="Heading4"
        label="Lock Level:"
        justify="Left"
        />
      <textmessage loc="16 32" size="64 32" style="Heading4"
        label="0.4"
        statpar="lklvl"
        />
    </group>
    <textmessage loc="88 32" size="88 32" style="Heading4"
      label="Off"
      statpar="lock"
      />
    <group loc="88 0" size="88 32" style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      useref="no"
      >
      <toggle loc="40 0" size="40 32" style="Heading2"
        label="Off"
        vc="sethw('lock','off')"
        statpar="lockon"
        statval="false"
        statshow="true"
        radiobutton="true"
        />
      <toggle loc="0 0" size="40 32" style="Heading4"
        label="On"
        vc="sethw('lock','on')"
        statpar="lockon"
        statval="true"
        statshow="false"
        radiobutton="true"
        />
    </group>
    <group loc="224 0" size="152 24" style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      useref="no"
      >
      <toggle loc="0 0" size="72 24" style="Heading4"
        label="Insert"
        vc="insert"
        show="$VALUE=0"
        statpar="air"
        statval="insert"
        radiobutton="true"
        />
      <toggle loc="72 0" size="72 24" style="Heading4"
        label="Eject"
        vc="eject"
        show="$VALUE=0"
        statpar="air"
        statval="eject"
        radiobutton="true"
        />
    </group>
    <shimbutton loc="224 144" size="80 32" style="Label4"
      label="Phase"
      vc="setshim('lockphase',$VALUE)"
      vq2="lockphase"
      statpar="lockphase"
      pointy="false"
      rocker="true"
      arrow="true"
      arrowcolor="GraphForeground"
      wrap="true"
      />
    <shimbutton loc="224 112" size="80 32" style="Label4"
      label="Gain"
      vc="setshim('lockgain',$VALUE)"
      vq2="lockgain"
      statpar="lockgain"
      pointy="false"
      rocker="true"
      arrow="true"
      arrowcolor="GraphForeground"
      wrap="false"
      />
    <shimbutton loc="224 80" size="80 32" style="Label4"
      label="Power"
      vc="setshim('lockpower',$VALUE)"
      vq2="lockpower"
      statpar="lockpower"
      pointy="false"
      rocker="true"
      arrow="true"
      arrowcolor="GraphForeground"
      wrap="false"
      />
    <shimbutton loc="224 48" size="80 32" style="Label4"
      label="Z0"
      vc="setshim('z0',$VALUE)"
      vq2="z0"
      statpar="z0"
      pointy="false"
      rocker="true"
      arrow="true"
      arrowcolor="GraphForeground"
      wrap="false"
      />
    <slider loc="320 144" size="472 32" style="Label4"
      vc="setshim('lockphase',$VALUE)"
      digital="true"
      incr1="1"
      incr2="10"
      vq2="lockphase"
      digits="8"
      statpar="lockphase"
      drag="0"
      />
    <slider loc="320 112" size="472 32" style="Label4"
      vc="setshim('lockgain',$VALUE)"
      digital="true"
      incr1="1"
      incr2="3"
      vq2="lockgain"
      digits="8"
      statpar="lockgain"
      drag="0"
      />
    <slider loc="320 80" size="472 32" style="Label4"
      vc="setshim('lockpower',$VALUE)"
      digital="true"
      incr1="1"
      incr2="3"
      vq2="lockpower"
      digits="8"
      statpar="lockpower"
      drag="0"
      />
    <slider loc="320 48" size="472 32" style="Label4"
      vc="setshim('z0',$VALUE)"
      digital="true"
      incr1="1"
      incr2="10"
      vq2="z0"
      digits="8"
      statpar="z0"
      drag="0"
      />
    <group loc="608 0" size="184 48" style="PlainText"
      vq="system"
      show="is_spectrometer('value'):$ENABLE"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      useref="no"
      >
      <label loc="80 0" size="56 24" style="Label4"
        label="Spin:"
        show="$VALUE=0"
        justify="Left"
        />
      <label loc="136 24" size="40 24" style="Label4"
        label=" Hz"
        show="$VALUE=0"
        justify="Left"
        />
      <textmessage loc="136 0" size="40 24" style="Label4"
        label="Off"
        show="$VALUE=0"
        statpar="spin"
        />
      <textmessage loc="80 24" size="56 24" style="Label4"
        label="0"
        show="$VALUE=0"
        statpar="spinval"
        />
      <button loc="0 24" size="72 24" style="Label4"
        label="Spin Off"
        vc="sethw('spinner','off')"
        show="$VALUE=0"
        />
      <button loc="0 0" size="72 24" style="Label4"
        label="Spin On"
        vc="sethw('spin',spin)"
        show="$VALUE=0"
        />
    </group>
    <label loc="8 32" size="64 32" style="Heading4"
      label="Status:"
      justify="Left"
      />
    <label loc="8 0" size="64 32" style="Heading4"
      label="Lock:"
      justify="Left"
      />
  </group>
</template>
