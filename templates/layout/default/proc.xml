<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="xpoleditv1-Process" element="panels" type="processing" >
  <group loc="0 0" size="800 200" style="Label4"
    label="Process"
    vc="wft(1) on('ProcessF2Data1'):$ProcessF2Data1 if ($ProcessF2Data1&lt;0.5) then create('ProcessF2Data1') endif"
    vc2="on('ProcessF2Data1'):$ProcessF2Data1 if ($ProcessF2Data1&gt;0.5) then destroy('ProcessF2Data1') endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    reference="ProcessF2Data"
    useref="yes"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 200" style="Heading2"
    label="Digital Filters"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    reference="FilterF2Data"
    useref="yes"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 200" style="Label4"
    label="Referencing"
    vc="exists('Referencing1','parameter'):$Referencing1 if ($Referencing1&lt;0.5) then create('Referencing1')  Referencing1=1 off('Referencing1') endif"
    vc2="exists('Referencing1','parameter'):$Referencing1 if ($Referencing1&gt;0.5) then destroy('Referencing1')  endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    reference="Referencing"
    useref="yes"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 200" style="Label4"
    label="Process F1"
    vc="on('ProcessF1Data1'):$ProcessF1Data1 if ($ProcessF1Data1&lt;0.5) then create('ProcessF1Data1') endif"
    vc2="on('ProcessF1Data1'):$ProcessF1Data1 if ($ProcessF1Data1&gt;0.5) then destroy('ProcessF1Data1') endif"
    show="on('ni'):$VALUE if $VALUE&gt;-1 then if (ni&gt;1) then $VALUE=1 else $VALUE=0 endif else $VALUE=0 endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    reference="ProcessF1Data"
    useref="yes"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 200" style="Label4"
    label="Reference F1"
    vc="exists('Referencingf1','parameter'):$Referencingf1 if ($Referencingf1&lt;0.5) then create('Referencingf1')  Referencingf1=1 off('Referencingf1') endif"
    vc2="exists('Referencingf1','parameter'):$Referencingf1 if ($Referencingf1&gt;0.5) then destroy('Referencingf1')  endif"
    show="on('ni'):$VALUE if $VALUE&gt;-1 then if (ni&gt;1) then $VALUE=1 else $VALUE=0 endif else $VALUE=0 endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    reference="Referencingf1"
    useref="yes"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 200" style="Heading4"
    label="Display"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    reference="Display1D"
    useref="yes"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 200" style="Heading2"
    label="Line Lists"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    reference="LineLists"
    useref="yes"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 200" style="PlainText"
    label="Plot"
    bg="transparent"
    border="None"
    side="BelowTop"
    justify="Left"
    tab="yes"
    reference="Plot1D"
    useref="yes"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 200" style="PlainText"
    label="Variables"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    reference="Variables"
    useref="yes"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 200" style="StdPar"
    label="Text Output"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    reference="TextOutput"
    useref="yes"
    expanded="no"
    >
  </group>
</template>
